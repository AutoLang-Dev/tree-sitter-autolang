{
  "name": "tree-sitter-autolang",
  "version": "0.2.0",
  "description": "AutoLang Parser",
  "repository": "https://github.com/autolang-dev/tree-sitter-autolang",
  "license": "MIT",
  "author": {
    "name": "AutoLang-Dev",
    "email": "contact@autolang.dev",
    "url": "https://autolang.dev/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "autolang"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "@keqingmoe/tree-sitter": "^0.26.2",
    "tree-sitter-cli": "^0.25.10"
  },
  "peerDependencies": {
    "@keqingmoe/tree-sitter": "^0.26.2"
  },
  "peerDependenciesMeta": {
    "@keqingmoe/tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
